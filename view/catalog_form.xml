<form string="Catalog" col="8">
    <label name="name"/>
    <field name="name" xexpand="1"/>
    <label name="year"/>
    <field name="year"/>
    <label name="code"/>
    <field name="code"/>
    <newline />
    <field name="sessions" colspan="8"/>
	<label name="description"/>
	<field name="description"/>
	<group col="2" colspan="2" id="states">
		<label name="state"/>
        <field name="state"/>
	</group>
	<group col="6" colspan="2" id="buttons">
		<button name="cancel" string="Cancel"
                        icon="tryton-cancel"/>
		<button name="draft" string="Draft"/>
		<button name="open" string="Confirm"
                        icon="tryton-go-next"/>
		<button name="done" string="Done"
                        icon="tryton-go-next"/>
	</group>
</form>
